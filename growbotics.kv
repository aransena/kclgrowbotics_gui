Logic:
    BoxLayout:
        orientation: "vertical"
        BoxLayout:
            size_hint: [1, .1]
            TextInput:
                id: statusTxt
                font_size: 30
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                padding_x:[self.center[0] - self._get_text_width(max(self._lines, key=len), self.tab_width, self._label_cached) / 2.0,0] if self.text else [self.center[0], 0]
                padding_y: [self.height / 2.0 - (self.line_height / 2.0) * len(self._lines), 0]
                text: ""
                read_only: False

        GridLayout:
            id: grid
            cols: 4

            Button:
                id: slot1
                label: "slot1"
                bold: True
                on_press: root.process_control(slot1.label)#,filename.text)
                background_color: (255,255,255,1)
                Image:
                    id:im1
                    source: 'images/plant1.png'
                    size: self.parent.size
                    center_x: self.parent.center_x
                    center_y: self.parent.center_y
                    allow_stretch: False
